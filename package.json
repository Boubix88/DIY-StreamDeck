{
  "name": "diy-streamdeck",
  "version": "1.0.0",
  "description": "Application de contrôle StreamDeck personnalisée avec Electron et React",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"cross-env NODE_ENV=development electronmon .\" \"cross-env BROWSER=none npm run start:renderer\"",
    "start:renderer": "cross-env BROWSER=none react-app-rewired start",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron:build": "npm run build && electron-builder build --win --publish never",
    "electron:dev": "concurrently \"cross-env NODE_ENV=development electron .\" \"wait-on http://localhost:3000 && cross-env BROWSER=none npm run start:renderer\"",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boubix88/DIY-StreamDeck.git"
  },
  "keywords": [
    "streamdeck",
    "electron",
    "react",
    "typescript",
    "arduino",
    "serial"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Boubix88/DIY-StreamDeck/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cbor-x": "^1.4.3",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "loudness": "^0.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1",
    "serialport": "^10.5.0",
    "systeminformation": "^5.27.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react-icons": "^2.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/serialport": "^8.0.5",
    "@types/systeminformation": "^3.23.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.diy.streamdeck",
    "productName": "DIY StreamDeck",
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  },
  "package": {
    "version": "1.0.0",
    "name": "diy-streamdeck",
    "main": "dist/main/main.js",
    "scripts": {
      "start": "electron .",
      "dev": "concurrently \"cross-env NODE_ENV=development electronmon .\" \"cross-env BROWSER=none npm run start:renderer\"",
      "start:renderer": "cross-env BROWSER=none react-app-rewired start",
      "build": "npm run build:main && npm run build:renderer",
      "build:main": "tsc -p tsconfig.main.json",
      "build:renderer": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
      "test": "react-app-rewired test",
      "eject": "react-scripts eject",
      "electron:build": "npm run build && electron-builder build --win --publish never",
      "electron:dev": "concurrently \"cross-env NODE_ENV=development electron .\" \"wait-on http://localhost:3000 && cross-env BROWSER=none npm run start:renderer\"",
      "postinstall": "electron-builder install-app-deps",
      "lint": "eslint --ext .ts,.tsx,.js,.jsx src/"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/Boubix88/DIY-StreamDeck.git"
    },
    "keywords": [
      "streamdeck",
      "electron",
      "react",
      "typescript",
      "arduino",
      "serial"
    ],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
      "url": "https://github.com/Boubix88/DIY-StreamDeck/issues"
    },
    "homepage": "./"
  }
}
